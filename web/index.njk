 <script> 
        let rangevalue = document.getElementById("myRange");
        let setvalue = document.getElementById("rangevalue");
        let imgchangevalue = document.getElementById("imgchange")

        rangevalue.oninput = function() {
            let getvalue = rangevalue.value;
            setvalue.innerHTML = this.value;
            if (getvalue == 2) {
                imgchangevalue.setAttribute("src", "images/2.png")
            } else if (getvalue == 3) {
                imgchangevalue.setAttribute("src", "images/3.png")
            } else if (getvalue == 4) {
                imgchangevalue.setAttribute("src", "images/4.png")
            } else if (getvalue == 5) {
                imgchangevalue.setAttribute("src", "images/5.png")
            } else if (getvalue == 6) {
                imgchangevalue.setAttribute("src", "images/6.png")
            } else if (getvalue == 1) {
                imgchangevalue.setAttribute("src", "images/1.png")
            }
        }
  </script>